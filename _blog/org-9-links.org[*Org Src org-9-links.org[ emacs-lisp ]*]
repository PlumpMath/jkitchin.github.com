  "file-hydra"
  ("e" (find-file f) "Open in emacs"))

(org-link-set-parameters "file" :follow 'file-hydra/body)


(message "%s" (file-hydra/body))
'

(let ((lexical-binding t)
      (f "hy-test.png"))
  (file-hydra/body)
  
  )

