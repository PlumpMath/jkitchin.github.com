(ivy-read "Open: " (emacsql org-db [:select [files:filename links:begin]
				    :from links :inner :join files :on (= links:filename-id files:rowid) 
				    :where (and (= type "cite") (like path "%kitchin-2015-examp%"))])
	  :action '(1 ("o"
		       (lambda (c) (message "%s" c)
			 (find-file (car c))
			 (goto-char (nth 1 c))))))
