from bs4 import BeautifulSoup

with open('cas.html') as f:
    html = BeautifulSoup(f.read())

data = {}

rows = html.find_all('tr')

while len(rows) > 1:
    row = rows.pop().find_all('td')
    print(row)
    if row[0].text == '\xa0':
        data[title] = {'coden': coden,
                         'abbrev': [abbrv]}

    # full title row
    if row[1].text == ' ':
        title = row[0].text
    else:
        abbrv = row[0].text
        coden = row[1].text

print(data)
