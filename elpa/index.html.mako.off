<%inherit file="../_templates/site.mako" />
<article class="page_box">
<%self:filter chain="markdown">

<%!
    import os
%>
<ul>
<li><a href="archive-contents">archive-contents</a></li>
% for f in os.listdir('elpa'):
% if f.endswith('.tar') or f.endswith('.el') or f.endswith('.org'):
  <li><a href="${f}">${f}</a></li>
% endif
% endfor
</ul>


</%self:filter>
</article>
